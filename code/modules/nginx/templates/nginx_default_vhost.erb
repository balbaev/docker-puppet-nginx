server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name _;

	location / {
                default_type 'text/html';
                content_by_lua '
                  ngx.say("Hello <b><%= @subject %></b>!")
                ';
	}

}
